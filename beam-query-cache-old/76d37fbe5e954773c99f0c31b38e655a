SELECT "t0"."GenreId" AS "res0",
       "t0"."Name" AS "res1",
       COUNT(DISTINCT "t1"."UnitPrice") AS "res2",
       (SUM(ALL "t1"."Milliseconds")) / (1000) AS "res3"
FROM "Genre" AS "t0"
CROSS JOIN "Track" AS "t1"
GROUP BY "t0"."GenreId",
         "t0"."Name"
HAVING ((SUM(ALL "t1"."Milliseconds")) / (1000)) >= (300000)