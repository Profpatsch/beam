UPDATE "cart_users"
SET "first_name"=?,
    "last_name"=?,
    "password"=?
WHERE ("email")=(?) -- With values: [SQLText "James",SQLText "Smith",SQLText "52a516ca6df436828d9c0d26e31ef704",SQLText "james@example.com"]

  SELECT "t0"."email" AS "res0",
         "t0"."first_name" AS "res1",
         "t0"."last_name" AS "res2",
         "t0"."password" AS "res3"
  FROM "cart_users" AS "t0" WHERE ("t0"."email")=(?) -- With values: [SQLText "james@example.com"]
