(
   (SELECT "t0"."FirstName" AS "res0"
    FROM "Customer" AS "t0")
 UNION
   (SELECT "t0"."FirstName" AS "res0"
    FROM "Employee" AS "t0")) INTERSECT (
                                           (SELECT "t0"."LastName" AS "res0"
                                            FROM "Customer" AS "t0")
                                         EXCEPT
                                           (SELECT "t0"."LastName" AS "res0"
                                            FROM "Employee" AS "t0"))