SELECT "t0"."res0" AS "res0",
       "t0"."res1" AS "res1"
FROM
  (SELECT "t0"."FirstName" AS "res0",
          "t0"."LastName" AS "res1"
   FROM "Customer" AS "t0"
   LIMIT 10) AS "t0"
UNION
SELECT "t0"."res0" AS "res0",
       "t0"."res1" AS "res1"
FROM
  (SELECT "t0"."FirstName" AS "res0",
          "t0"."LastName" AS "res1"
   FROM "Employee" AS "t0"
   LIMIT 10) AS "t0" -- With values: []
